{
  "name": "labofolio-front",
  "version": "1.0.0",
  "description": "``` labofolio-front = svelte + web-api sample ```",
  "main": "dist/bundle.js",
  "dependencies": {
    "dotenv": "^8.0.0"
  },
  "devDependencies": {
    "commander": "^2.20.0",
    "eslint": "^6.1.0",
    "eslint-plugin-svelte3": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "pug-cli": "^1.0.0-alpha6",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "svelte": "^3.6.7"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "SERVICE_MODE=build run-s build:clean build:prepare build:html build:rollup",
    "build:clean": "rm -rf dist",
    "build:prepare": "mkdir -p dist",
    "build:html": "pug public/index.pug --out dist",
    "build:rollup": "rollup -c",
    "start": "SERVICE_MODE=watch run-s build:clean build:prepare build:html build:watch",
    "build:watch": "rollup -c -w",
    "eslint": "eslint --ext .js ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foway0/labofolio-front.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foway0/labofolio-front/issues"
  },
  "homepage": "https://github.com/foway0/labofolio-front#readme",
  "directories": {
    "test": "test"
  },
  "eslintIgnore": ["dist"]
}
